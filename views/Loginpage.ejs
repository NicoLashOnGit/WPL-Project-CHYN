<%- include("partials/header") %>

<main id="Login">
    <article id="LoginMenu">
    
    <img src="/Images/Logos/fn_logo_met_hamer_copy.png" alt="" id="loginLogo">
    <h2 id="LoginMenuTitle">Sign In</h2>
    <form id="loginForm">
        <label for="email">Email Address:</label><br>
        <input type="text" id="email" required><br>
      
        <label for="password">Password:</label><br>
        <input type="password" id="password" required>
    </form>
        <a href="#" class="loginLink">Forgot Password?</a> 
    
        <button id="LogInButton" type="submit">Log in</button>

        <section id="socialsLogins">
            <button id="facebookLogin" class="SocialsLoginBtns">
                <img src="Images/Icons/Facebook-64.png" class="SocialsLoginImages">
            </button>
            <button id="InstagramLogin" class="SocialsLoginBtns">
                <img src="Images/Icons/Instagram-64.png" class="SocialsLoginImages">
            </button>
            <button id="TwitterLogin" class="SocialsLoginBtns">
                <img src="Images/Icons/Twitter-64.png" class="SocialsLoginImages">
            </button>
            <button id="YoutubeLogin" class="SocialsLoginBtns">
                <img src="Images/Icons/Whatsapp-64.png" class="SocialsLoginImages">
            </button>
        </section>

        <a href="#" class="loginLink">Create Account</a>
        <a href="#" class="loginLink">Privacy Policy</a>
    </article>
</main>

<%- include("partials/footer") %>